pipeline {
agent any

stages {
   stage ('build') {
     steps {
     sh 'ant -f build.xml -v'
       echo 'building..'
          }
        }
       }
     stage ('test') {
       steps {
         echo 'testing'
         }
         } 
     stage ('deploy'){
     steps {
       echo 'deploying...'
     post {
       always {
        archive 'dist/*.jar'
      }
     post {
    always {
     archive 'dist/* .jar'
      }
     
     }
    } 
   }
  
 
